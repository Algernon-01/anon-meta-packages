make_lintian_hook_pre() {
   if [ "$genmkfile_lintian_pre_opts" = "" ]; then
      genmkfile_lintian_pre_opts="--include-dir $GENMKFILE_ROOT_DIR --profile whonix"
   else
      genmkfile_lintian_pre_opts+="$genmkfile_lintian_pre_opts --include-dir $GENMKFILE_ROOT_DIR --profile whonix"
   fi
}
